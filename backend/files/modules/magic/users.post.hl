.arguments
   username:string
   password:string
auth.ticket.verify:admin
eval:x:+
slots.signal:transformers.hash-password
   reference:x:@.arguments/*/password
add:x:./*/wait.slots.signal/*/values
   get-nodes:x:@.arguments/*
wait.slots.signal:magic.db.mysql.create
   database:magic_publishing
   table:users
   return-id:bool:false
   values
slots.return-nodes:x:@wait.slots.signal/*
