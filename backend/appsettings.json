{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "magic": {
    "staticFiles": {
      "maxAge":  300
    },
    "databases": {
      "mysql": {
        "generic": "Server=127.0.0.1;Database={database};Uid=root;Pwd=ThisIsNotANicePassword;SslMode=Preferred;Old Guids=true;"
      }
    },
    "auth": {
      "secret": "Please change this, please change this, this is not a safe JWT token - Hence, please change it!",
      "valid-minutes": 120
    },
    "io": {
      "root-folder": "~/files/"
    },
    "endpoint": {
      "root-folder": "~/files/"
    },
    "scheduler": {
      "tasks-folder": "~/tasks/",
      "auto-start": true,
      "max-threads": 4
    }
  }
}